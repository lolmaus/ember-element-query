{
  "name": "ember-element-query",
  "version": "4.1.1",
  "description": "Use element queries effortlessly on any element or component. Make responsive components with CSS transformations and/or template transfomrations.",
  "keywords": [
    "ember-addon",
    "element-query",
    "element-queries",
    "container-query",
    "container-queries",
    "eq",
    "e-q",
    "responsive-web-design",
    "responsive",
    "rwd",
    "element query",
    "element queries",
    "container query",
    "container queries",
    "eq",
    "responsive web design",
    "ember",
    "sass",
    "scss",
    "slice",
    "slices",
    "breakpoint-slicer",
    "breakpoint slicer",
    "media query",
    "media-query",
    "media queries",
    "media-queries"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lolmaus/ember-element-query.git"
  },
  "license": "MIT",
  "author": "Andrey Mikhaylov (lolmaus) <https://github.com/lolmaus/",
  "homepage": "https://github.com/lolmaus/ember-element-query/",
  "bugs": "https://github.com/lolmaus/ember-element-query/issues/",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:ts": "tsc --noEmit",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint . --ignore-pattern '**/typedoc-theme/**'",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint-staged": "lint-staged",
    "lint:hbs:fix": "yarn lint:hbs --fix",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "release": "release-it",
    "typedoc": "typedoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "ember-auto-import": "^2.4.1",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-htmlbars": "5.7.1",
    "ember-cli-typescript": "^4.2.1",
    "ember-element-helper": "^0.8.5",
    "ember-lifeline": "^6.0.2",
    "ember-modifier": "^4.1.0",
    "ember-resize-observer-service": "^0.3.0",
    "ember-window-mock": "^0.7.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/test-helpers": "^2.2.5",
    "@embroider/test-setup": "^0.42.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.9",
    "@types/ember-test-helpers": "^1.0.8",
    "@types/ember__test-helpers": "^2.0.0",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "d3-array": "^3.0.1",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.0.1",
    "ember-cli": "~3.26.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-css-modules": "^1.5.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.26.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.3.3",
    "ember-template-lint": "^3.2.0",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-ember": "^10.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.10.0",
    "release-it-lerna-changelog": "^3.1.0",
    "sinon": "^11.1.1",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.4",
    "webpack": "^5.70.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://lolmaus.github.io/ember-element-query/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.4"
  }
}
